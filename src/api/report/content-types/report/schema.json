{
  "kind": "collectionType",
  "collectionName": "reports",
  "info": {
    "singularName": "report",
    "pluralName": "reports",
    "displayName": "Report",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Name": {
      "type": "string"
    },
    "Description": {
      "type": "richtext"
    },
    "Image": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "Start_Date": {
      "type": "date"
    },
    "End_Date": {
      "type": "date"
    },
    "Report_type": {
      "type": "enumeration",
      "enum": [
        "material",
        "follow"
      ]
    },
    "Favorite": {
      "type": "boolean"
    },
    "Steps": {
      "type": "enumeration",
      "enum": [
        "Indoor finishing work",
        "structural work",
        "finishing touches",
        "finition",
        "outdoor design",
        "key reception",
        "house delivery"
      ]
    },
    "Report_owner": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "project": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::project.project",
      "inversedBy": "Reports"
    }
  }
}
