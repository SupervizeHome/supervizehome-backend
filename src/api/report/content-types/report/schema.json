{
  "kind": "collectionType",
  "collectionName": "reports",
  "info": {
    "singularName": "report",
    "pluralName": "reports",
    "displayName": "Report",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "description": {
      "type": "richtext"
    },
    "image": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "start_date": {
      "type": "date"
    },
    "end_date": {
      "type": "date"
    },
    "report_type": {
      "type": "enumeration",
      "enum": [
        "material",
        "follow"
      ]
    },
    "favorite": {
      "type": "boolean"
    },
    "steps": {
      "type": "enumeration",
      "enum": [
        "Indoor finishing work",
        "structural work",
        "finishing touches",
        "finition",
        "outdoor design",
        "key reception",
        "house delivery"
      ]
    },
    "report_owner": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "project": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::project.project",
      "inversedBy": "reports"
    }
  }
}
