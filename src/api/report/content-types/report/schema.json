{
  "kind": "collectionType",
  "collectionName": "reports",
  "info": {
    "singularName": "report",
    "pluralName": "reports",
    "displayName": "Report",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "richtext",
      "required": true
    },
    "images": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "start_date": {
      "type": "date"
    },
    "report_type": {
      "type": "enumeration",
      "enum": [
        "material",
        "follow"
      ]
    },
    "favorite": {
      "type": "boolean"
    },
    "step": {
      "type": "enumeration",
      "enum": [
        "travaux de finition intérieure",
        "gros œuvre",
        "second œuvre",
        "finition",
        "conception extérieure",
        "réception des clés"
      ]
    },
    "project": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::project.project",
      "inversedBy": "reports"
    },
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "reports"
    },
    "files": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "files"
      ]
    }
  }
}
